<% if tile.character != nil %>
  <% if tile.character.tile_id === @character.tile_id  %>
  <%= image_tag "robo64", class: "actor" %>
  <% end %>
<% end %>
<% if tile.item != nil %>
  <% if tile.item.obtainable === true %>
    <%= link_to character_item_path(@character, tile.item), remote:true do %>
    <%= image_tag "#{tile.item.img}", class: "item" %>
    <% end %>
  <% end %>
<% end %>
<% if tile.door != nil %>
  <% if tile.door.is_locked === true %>
  <%=link_to door_path(tile.door), remote:true do %>
    <%= image_tag "#{tile.door.img}", class: "door" %>
    <% end %>
  <% end %>
<% end %>
